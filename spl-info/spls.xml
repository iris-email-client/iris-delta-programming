<?xml version="1.0" encoding="UTF-8"?>
<spls xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../bin/spls.xsd">
	<spl name="IrisDeltaJ">
		<features>
			<feature name ="Base" />
			<feature name ="Persistence" />
			<feature name ="RelationalDB" />
			<feature name ="LuceneDB" />
			<feature name ="UI" />
			<feature name ="Console" />
			<feature name ="Window" />
			<feature name ="AddressBook" />
		</features>
		<configurations>
			<configuration formula="Base AND Persistence AND UI OR AddressBook" />
			<configuration formula="Persistence AND (RelationalDB XOR LuceneDB)" />
			<configuration formula="UI AND (Console XOR Window)" />
		</configurations>
		<deltas>
			<partition>
				<modul delta="dBase" when="Base" />
				<modul delta="dBaseCommand" when="Base" />
				<modul delta="dBaseException" when="Base" />
				<modul delta="dBaseModel" when="Base" />
				<modul delta="dBasePersistence" when="Base" />
				<modul delta="dBaseMail" when="Base" />
				<modul delta="dGmailProvider" when="Base" />
				<modul delta="dBaseFacade" when="Base" />
			</partition>
			<partition>
				<modul delta="dPersistenceSqlite" when="RelationalDB" />
			</partition>
			<partition>
				<modul delta="dPersistenceLucene" when="LuceneDB" />
			</partition>
			<partition>
				<modul delta="dAddressBook" when="RelationalDB AND AddressBook" />
				<modul delta="dAddressBookRelational" when="RelationalDB AND AddressBook" />
				<modul delta="dAddressBookConsole" when="RelationalDB AND AddressBook" />
				<modul delta="dAddressBookFacade" when="RelationalDB AND AddressBook" />
				<modul delta="dTestAddressBookDAO" when="RelationalDB AND AddressBook" />
			</partition>
			<partition>
				<modul delta="dAddressBook" when="LuceneDB AND AddressBook" />
				<modul delta="dAddressBookLucene" when="LuceneDB AND AddressBook" />
				<modul delta="dAddressBookConsole" when="LuceneDB AND AddressBook" />
				<modul delta="dAddressBookFacade" when="LuceneDB AND AddressBook" />
			</partition>
			<partition>
				<modul delta="dConsole" when="Console" />
			</partition>
		</deltas>
	</spl>
</spls>
